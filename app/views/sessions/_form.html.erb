<%= form_tag sessions_path %>
  <%= label_tag :staff_id, '姓名' %>
  <%= select_tag :staff_id, options_from_collection_for_select(@staffs, :id, :name), include_blank: " " %>

  <%= label_tag :truck_id, "車牌" %>
  <%= select_tag :truck_id, options_from_collection_for_select(@trucks, :id, :truck_id), include_blank: " " %>

  <%= submit_tag "登入" %>